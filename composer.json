{
  "name": "sigmarius/laravel-app-settings",
  "description": "Managing Laravel Application settings and Application versions",
  "version": "1.0.0",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Sigmarius\\LaravelAppSettings\\": "src/"
    }
  },
  "autoload-dev": {
